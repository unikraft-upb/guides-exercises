$(eval $(call addlib_s,libuktest,$(CONFIG_LIBUKTEST)))

CINCLUDES-$(CONFIG_LIBUKTEST)   += -I$(LIBUKTEST_BASE)/include
CXXINCLUDES-$(CONFIG_LIBUKTEST) += -I$(LIBUKTEST_BASE)/include

LIBUKTEST_SUPPRESS_FLAGS-y   += -Wno-unused-function
LIBUKTEST_SUPPRESS_FLAGS-y   += -Wno-unused-parameter
LIBUKTEST_CFLAGS-y           += $(LIBUKTEST_SUPPRESS_FLAGS-y)

LIBUKTEST_SRCS-y += $(LIBUKTEST_BASE)/test.c
LIBUKTEST_SRCS-y += $(LIBUKTEST_BASE)/tests.ld
